{
  "name":"oneup/contao-sfdump",
  "description":"This Contao Open Source CMS extension gives you the ability to use the Symfony VarDumper component in Contao with sfdump().",
  "keywords":["contao", "symfony", "debug", "dump", "module"],
  "type":"contao-module",
  "homepage":"http://1up.io",
  "license":"MIT",
  "authors":[
    {
      "name":"David Greminger",
      "email":"dg@1up.io",
      "homepage":"http://1up.io",
      "role":"Developer"
    }
  ],
  "support":{
    "email":"hello@1up.io",
    "issues":"https://github.com/1up-lab/contao-sfdump/issues",
    "irc":"irc://irc.freenode.org/contao.dev",
    "source":"https://github.com/1up-lab/contao-sfdump"
  },
  "require":{
    "php":">=5.6",
    "contao/core-bundle":"^3.2 || ^4.1",
    "contao-community-alliance/composer-plugin":"~2.4 | ~3.0",
    "symfony/var-dumper": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Oneup\\Contao\\": "src"
    }
  },
  "extra": {
    "contao": {
      "sources": {
        "contao": "system/modules/sfdump"
      }
    }
  },
  "prefer-stable": true
}
